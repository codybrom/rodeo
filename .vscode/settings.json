{
	"files.exclude": {
		"out": false
	},
	"search.exclude": {
		"out": true
	},
	"typescript.tsdk": "node_modules/typescript/lib",
	"cSpell.words": [
		"codybrom",
		"dqbd",
		"esbuild",
		"haml",
		"ipynb",
		"podspec",
		"rodeoignore",
		"tiktoken",
		"unmark",
		"xcconfig",
		"yarnrc"
	],
	"gpt-context-generator.ignoreFiles": [
		".gitignore",
		".dockerignore",
		".rodeoignore"
	],
	"cSpell.enabled": true,
	"testing.automaticallyOpenPeekView": "failureInVisibleDocument",
	"testing.defaultGutterClickAction": "run",
	"testing.followRunningTest": true,
	"testing.showAllMessages": false,
	"jest.runMode": {
		"coverage": true,
		"type": "watch",
	}
}